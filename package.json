  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mindiply/conditional-ga/issues"
  },
  "homepage": "https://github.com/mindiply/conditional-ga#readme",
  "devDependencies": {
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
