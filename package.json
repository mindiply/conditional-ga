{
  "name": "conditional-ga",
  "version": "0.0.12",
  "description": "Use google Analytics helpers, doing the actual event firing only if enabled via either DOM or client side tests",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && babel src --out-dir lib",
    "build_watch": "npm run clean && babel src -w --out-dir lib",
    "lint": "eslint src",
    "test": "npm run lint",
    "prepublish": "npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mindiply/conditional-ga.git"
  },
  "keywords": [
    "google",
    "analytics"
  ],
  "author": "Paolo F. Bongiovanni <paolo.bongiovanni@mindiply.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mindiply/conditional-ga/issues"
  },
  "homepage": "https://github.com/mindiply/conditional-ga#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "rimraf": "^2.6.2"
  }
}
